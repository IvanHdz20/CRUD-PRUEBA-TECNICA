pan>\r\n            </td>\r\n            <td class=\"fw-bold align-middle\">{{ item.Nombre }}</td>\r\n            <td class=\"text-center align-middle\">\r\n              <span class=\"badge bg-light text-dark\">{{ item.Sku }}</span>\r\n            </td>\r\n            <td class=\"text-center align-middle\">\r\n              <span class=\"text-success fw-bold\">${{ item.Precio }}</span>\r\n            </td>\r\n            <td class=\"text-center align-middle\">\r\n              <span :class=\"getStockClass(item.Stock)\">{{ item.Stock }}</span>\r\n            </td>\r\n            <td class=\"text-center align-middle\">\r\n              <span class=\"badge bg-info\">{{ item.Categoria }}</span>\r\n            </td>\r\n            <td class=\"text-center align-middle\">\r\n              <span :class=\"item.Activo ? 'badge bg-success' : 'badge bg-danger'\">\r\n                {{ item.Activo ? 'Activo' : 'Inactivo' }}\r\n              </span>\r\n            </td>\r\n            <td class=\"text-center align-middle\">\r\n              <div class=\"btn-group btn-group-sm\">\r\n                <button class=\"btn btn-primary\" @click=\"$emit('editar', item.Id)\" title=\"Editar\">\r\n                  <i class=\"bi bi-pencil\"></i>\r\n                </button>\r\n                <button class=\"btn btn-danger\" @click=\"$emit('eliminar', item.Id)\" title=\"Eliminar\">\r\n                  <i class=\"bi bi-trash\"></i>\r\n                </button>\r\n              </div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"ProductoTable\",\r\n  props: {\r\n    productos: {\r\n      type: Array,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      search: ''\r\n    }\r\n  },\r\n  computed: {\r\n    filteredProductos() {\r\n      if (!this.search) return this.productos;\r\n      return this.productos.filter(p => \r\n        p.Nombre.toLowe