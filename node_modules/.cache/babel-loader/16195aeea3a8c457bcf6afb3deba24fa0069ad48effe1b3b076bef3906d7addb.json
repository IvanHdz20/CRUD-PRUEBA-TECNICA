{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: \"ProductoTable\",\n  props: {\n    productos: {\n      type: Array,\n      required: true\n    }\n  },\n  data() {\n    return {\n      busqueda: \"\",\n      headers: [{\n        title: \"ID\",\n        key: \"Id\"\n      }, {\n        title: \"Nombre\",\n        key: \"Nombre\"\n      }, {\n        title: \"SKU\",\n        key: \"Sku\"\n      }, {\n        title: \"Precio\",\n        key: \"Precio\"\n      }, {\n        title: \"Stock\",\n        key: \"Stock\"\n      }, {\n        title: \"Categoría\",\n        key: \"Categoria\"\n      }, {\n        title: \"Activo\",\n        key: \"Activo\"\n      }, {\n        title: \"Acciones\",\n        key: \"actions\",\n        sortable: false\n      }]\n    };\n  },\n  computed: {\n    filtrados() {\n      if (!this.busqueda) return this.productos;\n      const term = this.busqueda.toLowerCase();\n      return this.productos.filter(p => p.Nombre.toLowerCase().includes(term) || p.Sku.toLowerCase().includes(term) || p.Categoria.toLowerCase().includes(term));\n    }\n  }\n};","map":{"version":3,"names":["name","props","productos","type","Array","required","data","busqueda","headers","title","key","sortable","computed","filtrados","term","toLowerCase","filter","p","Nombre","includes","Sku","Categoria"],"sources":["C:\\Users\\Elpap\\OneDrive\\Desktop\\CRUD\\productos-vue\\src\\components\\ProductoTable.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <!-- Buscador -->\r\n    <v-text-field\r\n      v-model=\"busqueda\"\r\n      label=\"Buscar productos...\"\r\n      append-inner-icon=\"mdi-magnify\"\r\n      variant=\"outlined\"\r\n      density=\"compact\"\r\n      class=\"mb-4\"\r\n    ></v-text-field>\r\n\r\n    <!-- Tabla de productos -->\r\n    <v-data-table\r\n      :headers=\"headers\"\r\n      :items=\"filtrados\"\r\n      class=\"elevation-1\"\r\n      density=\"compact\"\r\n    >\r\n      <!-- Personalizamos solo las filas -->\r\n      <template v-slot:item=\"{ item }\">\r\n        <tr>\r\n          <td style=\"padding: 8px; text-align: center;\">{{ item.Id }}</td>\r\n          <td style=\"padding: 8px;\">{{ item.Nombre }}</td>\r\n          <td style=\"padding: 8px; text-align: center;\">{{ item.Sku }}</td>\r\n          <td style=\"padding: 8px; text-align: right;\">{{ item.Precio }}</td>\r\n          <td style=\"padding: 8px; text-align: center;\">{{ item.Stock }}</td>\r\n          <td style=\"padding: 8px;\">{{ item.Categoria }}</td>\r\n          <td style=\"padding: 8px; text-align: center;\">{{ item.Activo ? 'Sí' : 'No' }}</td>\r\n          <td style=\"padding: 8px; text-align: center;\">\r\n            <v-btn icon=\"mdi-pencil\" color=\"blue\" size=\"small\" @click=\"$emit('editar', item.Id)\">\r\n            </v-btn>\r\n            <v-btn icon=\"mdi-delete\" color=\"red\" size=\"small\" @click=\"$emit('eliminar', item.Id)\">\r\n            </v-btn>\r\n          </td>\r\n        </tr>\r\n      </template>\r\n    </v-data-table>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"ProductoTable\",\r\n  props: {\r\n    productos: {\r\n      type: Array,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      busqueda: \"\",\r\n      headers: [\r\n        { title: \"ID\", key: \"Id\" },\r\n        { title: \"Nombre\", key: \"Nombre\" },\r\n        { title: \"SKU\", key: \"Sku\" },\r\n        { title: \"Precio\", key: \"Precio\" },\r\n        { title: \"Stock\", key: \"Stock\" },\r\n        { title: \"Categoría\", key: \"Categoria\" },\r\n        { title: \"Activo\", key: \"Activo\" },\r\n        { title: \"Acciones\", key: \"actions\", sortable: false },\r\n      ]\r\n    }\r\n  },\r\n  computed: {\r\n    filtrados() {\r\n      if (!this.busqueda) return this.productos;\r\n      const term = this.busqueda.toLowerCase();\r\n      return this.productos.filter(p =>\r\n        p.Nombre.toLowerCase().includes(term) ||\r\n        p.Sku.toLowerCase().includes(term) ||\r\n        p.Categoria.toLowerCase().includes(term)\r\n      );\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":";;AA0CA,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,CACP;QAAEC,KAAK,EAAE,IAAI;QAAEC,GAAG,EAAE;MAAK,CAAC,EAC1B;QAAED,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE;MAAS,CAAC,EAClC;QAAED,KAAK,EAAE,KAAK;QAAEC,GAAG,EAAE;MAAM,CAAC,EAC5B;QAAED,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE;MAAS,CAAC,EAClC;QAAED,KAAK,EAAE,OAAO;QAAEC,GAAG,EAAE;MAAQ,CAAC,EAChC;QAAED,KAAK,EAAE,WAAW;QAAEC,GAAG,EAAE;MAAY,CAAC,EACxC;QAAED,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE;MAAS,CAAC,EAClC;QAAED,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,SAAS;QAAEC,QAAQ,EAAE;MAAM,CAAC;IAE1D;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,SAASA,CAAA,EAAG;MACV,IAAI,CAAC,IAAI,CAACN,QAAQ,EAAE,OAAO,IAAI,CAACL,SAAS;MACzC,MAAMY,IAAG,GAAI,IAAI,CAACP,QAAQ,CAACQ,WAAW,CAAC,CAAC;MACxC,OAAO,IAAI,CAACb,SAAS,CAACc,MAAM,CAACC,CAAA,IAC3BA,CAAC,CAACC,MAAM,CAACH,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,IAAI,KACpCG,CAAC,CAACG,GAAG,CAACL,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,IAAI,KACjCG,CAAC,CAACI,SAAS,CAACN,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,IAAI,CACzC,CAAC;IACH;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}