ductoTable.vue"],"sourcesContent":["<template>\r\n  <v-container class=\"pa-0\">\r\n    <!-- Buscador -->\r\n    <v-row class=\"mb-4\" align=\"center\">\r\n      <v-col cols=\"12\" md=\"6\">\r\n        <v-text-field\r\n          v-model=\"busqueda\"\r\n          label=\"Buscar por nombre / sku / categoría\"\r\n          append-icon=\"mdi-magnify\"\r\n          outlined\r\n          dense\r\n          clearable\r\n        />\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <!-- Tabla de productos -->\r\n    <v-data-table\r\n      :headers=\"headers\"\r\n      :items=\"filtrados\"\r\n      class=\"elevation-1 tabla-productos\"\r\n      dense\r\n    >\r\n      <!-- Personalizamos SOLO las filas (EL HEADER lo genera Vuetify automáticamente) -->\r\n      <template #item=\"{ item }\">\r\n        <tr>\r\n          <td class=\"col-id\">{{ item.Id }}</td>\r\n          <td class=\"col-nombre\">{{ item.Nombre }}</td>\r\n          <td class=\"col-sku\">{{ item.Sku }}</td>\r\n          <td class=\"col-precio\">${{ formatPrice(item.Precio) }}</td>\r\n          <td class=\"col-stock\">{{ item.Stock }}</td>\r\n          <td class=\"col-categoria\">{{ item.Categoria }}</td>\r\n          <td class=\"col-activo\">{{ item.Activo ? \"Sí\" : \"No\" }}</td>\r\n          <td class=\"col-acciones\">\r\n            <v-btn small icon @click=\"$emit('editar', item)\">\r\n              <v-icon>mdi-pencil</v-icon>\r\n            </v-btn>\r\n            <v-btn small icon @click=\"$emit('eliminar', item.Id)\">\r\n              <v-icon>mdi-delete</v-icon>\r\n            </v-btn>\r\n          </td>\r\n        </tr>\r\n      </template>\r\n    </v-data-table>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"ProductoTable\",\r\n  props: {\r\n    productos: {\r\n      type: Array,\r\n      required: true,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      busqueda: \"\",\r\n      headers: [\r\n        { text: \"ID\", value: \"Id\", align: \"center\" },\r\n        { text: \"Nombre\", value: \"Nombre\" },\r\n        { text: \"SKU\", value: \"Sku\", align: \"center\" },\r\n        { text: \"Precio\", value: \"Precio\", align: \"right\" },\r\n        { text: \"Stock\", value: \"Stock\", align: \"center\" },\r\n        { text: \"Categoría\", value: \"Categoria\" },\r\n        { text: \"Activo\", value: \"Activo\", align: \"center\" },\r\n        { text: \"Acciones\", value: \"actions\", sortable: false, align: \"center\" },\r\n      ],\r\n    };\r\n  },\r\n  computed: {\r\n    filtrados() {\r\n      if (!this.busqueda) return this.productos || [];\r\n      const term = this.busqueda.toLowerCase();\r\n      return (this.productos || []).filter((p) =>\r\n        (p.Nombre && p.Nombre.toString().toLowerCase().includes(term)) ||\r\n        (p.Sku && p.Sku.toString().toLowerCase().includes(term)) ||\r\n        (p.Categoria && p.Categoria.toString().toLowerCase().includes(term))\r\n      );\r\n    },\r\n  },\r\n  methods: {\r\n    formatPrice(val) {\r\n      if (val === null || val === undefined) return \"0.00\";\r\n      return Number(val).toFixed(2);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* ::v-deep para asegurarnos de apuntar al thead que genera Vuetify */\r\n::v-deep(.tabla-productos thead th) {\r\n  background-color: #f5f7fa !important; /* fondo claro */\r\n  color: #111 !important;                /* texto oscuro */\r\n  font-weight: 700;\r\n  text-align: center;\r\n  padding: 12px 8px !important;\r\n  border-bottom: 1px solid #e6e6e6;\r\n}\r\n\r\n/* Filas y celdas */\r\n::v-deep(.tabla-productos tbody td) {\r\n  padding: 12px 10px !important;\r\n  vertical-align: middle;\r\n  border-bottom: 1px solid #f1f1f1;\r\n  color: #222;\r\n}\r\n\r\n/* Anchos / alineaciones por columna para que no se amontone */\r\n.col-id { width: 64px; text-align: center; }\r\n.col-nombre { min-width: 220px; }\r\n.col-sku { width: 110px; text-align: center; }\r\n.col-precio { width: 110px; text-align: right; }\r\n.col-stock { width: 