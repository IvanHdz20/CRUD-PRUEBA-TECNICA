{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: \"ProductoTable\",\n  props: {\n    productos: {\n      type: Array,\n      required: true\n    }\n  },\n  data() {\n    return {\n      search: '',\n      headers: [{\n        text: \"ID\",\n        value: \"Id\"\n      }, {\n        text: \"Nombre\",\n        value: \"Nombre\"\n      }, {\n        text: \"SKU\",\n        value: \"Sku\"\n      }, {\n        text: \"Precio\",\n        value: \"Precio\"\n      }, {\n        text: \"Stock\",\n        value: \"Stock\"\n      }, {\n        text: \"Categor√≠a\",\n        value: \"Categoria\"\n      }, {\n        text: \"Activo\",\n        value: \"Activo\"\n      }, {\n        text: \"Acciones\",\n        value: \"actions\",\n        sortable: false\n      }]\n    };\n  },\n  computed: {\n    productosActivos() {\n      return this.productos.filter(p => p.Activo).length;\n    },\n    stockBajo() {\n      return this.productos.filter(p => p.Stock < 10).length;\n    },\n    filteredProductos() {\n      if (!this.search) return this.productos;\n      return this.productos.filter(producto => producto.Nombre.toLowerCase().includes(this.search.toLowerCase()) || producto.Sku.toLowerCase().includes(this.search.toLowerCase()) || producto.Categoria.toLowerCase().includes(this.search.toLowerCase()));\n    }\n  },\n  methods: {\n    formatearPrecio(precio) {\n      return new Intl.NumberFormat('es-MX', {\n        minimumFractionDigits: 2,\n        maximumFractionDigits: 2\n      }).format(precio);\n    },\n    getStockBadgeClass(stock) {\n      if (stock < 5) return 'badge bg-danger';\n      if (stock < 10) return 'badge bg-warning';\n      return 'badge bg-success';\n    }\n  }\n};","map":{"version":3,"names":["name","props","productos","type","Array","required","data","search","headers","text","value","sortable","computed","productosActivos","filter","p","Activo","length","stockBajo","Stock","filteredProductos","producto","Nombre","toLowerCase","includes","Sku","Categoria","methods","formatearPrecio","precio","Intl","NumberFormat","minimumFractionDigits","maximumFractionDigits","format","getStockBadgeClass","stock"],"sources":["C:\\Users\\Elpap\\OneDrive\\Desktop\\CRUD\\productos-vue\\src\\components\\ProductoTable.vue"],"sourcesContent":["<template>\r\n  <div class=\"container-fluid py-4\">\r\n    <!-- Header -->\r\n    <div class=\"row mb-4\">\r\n      <div class=\"col-md-8\">\r\n        <h3 class=\"mb-0 text-primary fw-bold\">\r\n          <i class=\"bi bi-box-seam me-2\"></i>\r\n          Gesti√≥n de Productos\r\n        </h3>\r\n      </div>\r\n      <div class=\"col-md-4\">\r\n        <div class=\"d-flex gap-2 flex-wrap justify-content-md-end\">\r\n          <span class=\"badge bg-primary\">{{ productos.length }} Total</span>\r\n          <span class=\"badge bg-success\">{{ productosActivos }} Activos</span>\r\n          <span class=\"badge bg-warning\">{{ stockBajo }} Stock Bajo</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Tabla -->\r\n    <div class=\"card shadow-sm border-0\">\r\n      <div class=\"card-header bg-white border-bottom-0 py-3\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-6\">\r\n            <h6 class=\"mb-0 text-muted\">Lista de Productos</h6>\r\n          </div>\r\n          <div class=\"col-md-6\">\r\n            <input \r\n              v-model=\"search\" \r\n              type=\"text\" \r\n              class=\"form-control form-control-sm\" \r\n              placeholder=\"üîç Buscar productos...\"\r\n            >\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n      <div class=\"card-body p-0\">\r\n        <v-data-table\r\n          :headers=\"headers\"\r\n          :items=\"filteredProductos\"\r\n          class=\"table-borderless\"\r\n          :items-per-page=\"10\"\r\n          hide-default-header\r\n          hide-default-footer\r\n        >\r\n          <template #headers>\r\n            <thead class=\"bg-light\">\r\n              <tr>\r\n                <th class=\"px-3 py-3 text-primary fw-semibold small\">ID</th>\r\n                <th class=\"px-3 py-3 text-primary fw-semibold small\">NOMBRE</th>\r\n                <th class=\"px-3 py-3 text-primary fw-semibold small\">SKU</th>\r\n                <th class=\"px-3 py-3 text-primary fw-semibold small\">PRECIO</th>\r\n                <th class=\"px-3 py-3 text-primary fw-semibold small\">STOCK</th>\r\n                <th class=\"px-3 py-3 text-primary fw-semibold small\">CATEGOR√çA</th>\r\n                <th class=\"px-3 py-3 text-primary fw-semibold small\">ESTADO</th>\r\n                <th class=\"px-3 py-3 text-primary fw-semibold small text-center\">ACCIONES</th>\r\n              </tr>\r\n            </thead>\r\n          </template>\r\n\r\n          <template #body=\"{ items }\">\r\n            <tbody>\r\n              <tr v-for=\"item in items\" :key=\"item.Id\" class=\"border-bottom\">\r\n                <!-- ID -->\r\n                <td class=\"px-3 py-3\">\r\n                  <span class=\"badge bg-light text-primary border\">#{{ item.Id }}</span>\r\n                </td>\r\n\r\n                <!-- Nombre -->\r\n                <td class=\"px-3 py-3\">\r\n                  <span class=\"fw-medium text-dark\">{{ item.Nombre }}</span>\r\n                </td>\r\n\r\n                <!-- SKU -->\r\n                <td class=\"px-3 py-3\">\r\n                  <span class=\"badge bg-secondary bg-opacity-25 text-dark\">{{ item.Sku }}</span>\r\n                </td>\r\n\r\n                <!-- Precio -->\r\n                <td class=\"px-3 py-3\">\r\n                  <span class=\"text-success fw-bold\">${{ formatearPrecio(item.Precio) }}</span>\r\n                </td>\r\n\r\n                <!-- Stock -->\r\n                <td class=\"px-3 py-3\">\r\n                  <span :class=\"getStockBadgeClass(item.Stock)\">\r\n                    {{ item.Stock }}\r\n                  </span>\r\n                </td>\r\n\r\n                <!-- Categor√≠a -->\r\n                <td class=\"px-3 py-3\">\r\n                  <span class=\"badge bg-info bg-opacity-25 text-dark\">\r\n                    <i class=\"bi bi-tag me-1\"></i>{{ item.Categoria }}\r\n                  </span>\r\n                </td>\r\n\r\n                <!-- Estado -->\r\n                <td class=\"px-3 py-3\">\r\n                  <span :class=\"item.Activo ? 'badge bg-success' : 'badge bg-danger'\">\r\n                    <i :class=\"item.Activo ? 'bi bi-check-circle me-1' : 'bi bi-x-circle me-1'\"></i>\r\n                    {{ item.Activo ? 'Activo' : 'Inactivo' }}\r\n                  </span>\r\n                </td>\r\n\r\n                <!-- Acciones -->\r\n                <td class=\"px-3 py-3 text-center\">\r\n                  <div class=\"btn-group btn-group-sm\">\r\n                    <button \r\n                      class=\"btn btn-outline-primary btn-sm\"\r\n                      @click=\"$emit('editar', item.Id)\"\r\n                      data-bs-toggle=\"tooltip\" \r\n                      title=\"Editar\"\r\n                    >\r\n                      <i class=\"bi bi-pencil\"></i>\r\n                    </button>\r\n                    <button \r\n                      class=\"btn btn-outline-danger btn-sm\"\r\n                      @click=\"$emit('eliminar', item.Id)\"\r\n                      data-bs-toggle=\"tooltip\" \r\n                      title=\"Eliminar\"\r\n                    >\r\n                      <i class=\"bi bi-trash\"></i>\r\n                    </button>\r\n                  </div>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </template>\r\n\r\n          <template #no-data>\r\n            <div class=\"text-center py-5\">\r\n              <i class=\"bi bi-box display-4 text-muted mb-3\"></i>\r\n              <p class=\"text-muted mb-0\">No hay productos disponibles</p>\r\n            </div>\r\n          </template>\r\n        </v-data-table>\r\n      </div>\r\n\r\n      <!-- Footer con paginaci√≥n simple -->\r\n      <div class=\"card-footer bg-light border-top-0\">\r\n        <div class=\"d-flex justify-content-between align-items-center\">\r\n          <small class=\"text-muted\">\r\n            Mostrando {{ Math.min(filteredProductos.length, 10) }} de {{ filteredProductos.length }} productos\r\n          </small>\r\n          <div class=\"text-muted small\">\r\n            <i class=\"bi bi-info-circle me-1\"></i>\r\n            Total de registros: {{ productos.length }}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"ProductoTable\",\r\n  props: {\r\n    productos: {\r\n      type: Array,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      search: '',\r\n      headers: [\r\n        { text: \"ID\", value: \"Id\" },\r\n        { text: \"Nombre\", value: \"Nombre\" },\r\n        { text: \"SKU\", value: \"Sku\" },\r\n        { text: \"Precio\", value: \"Precio\" },\r\n        { text: \"Stock\", value: \"Stock\" },\r\n        { text: \"Categor√≠a\", value: \"Categoria\" },\r\n        { text: \"Activo\", value: \"Activo\" },\r\n        { text: \"Acciones\", value: \"actions\", sortable: false },\r\n      ]\r\n    }\r\n  },\r\n  computed: {\r\n    productosActivos() {\r\n      return this.productos.filter(p => p.Activo).length;\r\n    },\r\n    stockBajo() {\r\n      return this.productos.filter(p => p.Stock < 10).length;\r\n    },\r\n    filteredProductos() {\r\n      if (!this.search) return this.productos;\r\n      \r\n      return this.productos.filter(producto => \r\n        producto.Nombre.toLowerCase().includes(this.search.toLowerCase()) ||\r\n        producto.Sku.toLowerCase().includes(this.search.toLowerCase()) ||\r\n        producto.Categoria.toLowerCase().includes(this.search.toLowerCase())\r\n      );\r\n    }\r\n  },\r\n  methods: {\r\n    formatearPrecio(precio) {\r\n      return new Intl.NumberFormat('es-MX', {\r\n        minimumFractionDigits: 2,\r\n        maximumFractionDigits: 2\r\n      }).format(precio);\r\n    },\r\n    getStockBadgeClass(stock) {\r\n      if (stock < 5) return 'badge bg-danger';\r\n      if (stock < 10) return 'badge bg-warning';\r\n      return 'badge bg-success';\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":";;AA6JA,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,CACP;QAAEC,IAAI,EAAE,IAAI;QAAEC,KAAK,EAAE;MAAK,CAAC,EAC3B;QAAED,IAAI,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAS,CAAC,EACnC;QAAED,IAAI,EAAE,KAAK;QAAEC,KAAK,EAAE;MAAM,CAAC,EAC7B;QAAED,IAAI,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAS,CAAC,EACnC;QAAED,IAAI,EAAE,OAAO;QAAEC,KAAK,EAAE;MAAQ,CAAC,EACjC;QAAED,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAY,CAAC,EACzC;QAAED,IAAI,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAS,CAAC,EACnC;QAAED,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE,SAAS;QAAEC,QAAQ,EAAE;MAAM,CAAC;IAE3D;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,OAAO,IAAI,CAACX,SAAS,CAACY,MAAM,CAACC,CAAA,IAAKA,CAAC,CAACC,MAAM,CAAC,CAACC,MAAM;IACpD,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,OAAO,IAAI,CAAChB,SAAS,CAACY,MAAM,CAACC,CAAA,IAAKA,CAAC,CAACI,KAAI,GAAI,EAAE,CAAC,CAACF,MAAM;IACxD,CAAC;IACDG,iBAAiBA,CAAA,EAAG;MAClB,IAAI,CAAC,IAAI,CAACb,MAAM,EAAE,OAAO,IAAI,CAACL,SAAS;MAEvC,OAAO,IAAI,CAACA,SAAS,CAACY,MAAM,CAACO,QAAO,IAClCA,QAAQ,CAACC,MAAM,CAACC,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACjB,MAAM,CAACgB,WAAW,CAAC,CAAC,KAChEF,QAAQ,CAACI,GAAG,CAACF,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACjB,MAAM,CAACgB,WAAW,CAAC,CAAC,KAC7DF,QAAQ,CAACK,SAAS,CAACH,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACjB,MAAM,CAACgB,WAAW,CAAC,CAAC,CACrE,CAAC;IACH;EACF,CAAC;EACDI,OAAO,EAAE;IACPC,eAAeA,CAACC,MAAM,EAAE;MACtB,OAAO,IAAIC,IAAI,CAACC,YAAY,CAAC,OAAO,EAAE;QACpCC,qBAAqB,EAAE,CAAC;QACxBC,qBAAqB,EAAE;MACzB,CAAC,CAAC,CAACC,MAAM,