{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: \"ProductoTable\",\n  props: {\n    productos: {\n      type: Array,\n      required: true\n    }\n  },\n  data() {\n    return {\n      busqueda: \"\",\n      headers: [{\n        title: \"ID\",\n        key: \"Id\",\n        align: \"center\"\n      }, {\n        title: \"Nombre\",\n        key: \"Nombre\",\n        align: \"start\"\n      }, {\n        title: \"SKU\",\n        key: \"Sku\",\n        align: \"center\"\n      }, {\n        title: \"Precio\",\n        key: \"Precio\",\n        align: \"end\"\n      }, {\n        title: \"Stock\",\n        key: \"Stock\",\n        align: \"center\"\n      }, {\n        title: \"Categoría\",\n        key: \"Categoria\",\n        align: \"start\"\n      }, {\n        title: \"Activo\",\n        key: \"Activo\",\n        align: \"center\"\n      }, {\n        title: \"Acciones\",\n        key: \"actions\",\n        sortable: false,\n        align: \"center\"\n      }]\n    };\n  },\n  computed: {\n    filtrados() {\n      if (!this.busqueda) return this.productos;\n      const term = this.busqueda.toLowerCase();\n      return this.productos.filter(p => p.Nombre.toLowerCase().includes(term) || p.Sku.toLowerCase().includes(term) || p.Categoria.toLowerCase().includes(term));\n    }\n  }\n};","map":{"version":3,"names":["name","props","productos","type","Array","required","data","busqueda","headers","title","key","align","sortable","computed","filtrados","term","toLowerCase","filter","p","Nombre","includes","Sku","Categoria"],"sources":["C:\\Users\\Elpap\\OneDrive\\Desktop\\CRUD\\productos-vue\\src\\components\\ProductoTable.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <!-- Buscador -->\r\n    <v-text-field\r\n      v-model=\"busqueda\"\r\n      label=\"Buscar productos...\"\r\n      append-inner-icon=\"mdi-magnify\"\r\n      variant=\"outlined\"\r\n      density=\"compact\"\r\n      class=\"mb-4\"\r\n    ></v-text-field>\r\n\r\n    <!-- Tabla de productos -->\r\n    <v-data-table\r\n      :headers=\"headers\"\r\n      :items=\"filtrados\"\r\n      class=\"elevation-1\"\r\n      density=\"compact\"\r\n    >\r\n      <!-- Personalizamos solo las filas -->\r\n      <template v-slot:item=\"{ item }\">\r\n        <tr>\r\n          <td style=\"padding: 8px; text-align: center;\">{{ item.Id }}</td>\r\n          <td style=\"padding: 8px;\">{{ item.Nombre }}</td>\r\n          <td style=\"padding: 8px; text-align: center;\">{{ item.Sku }}</td>\r\n          <td style=\"padding: 8px; text-align: right;\">${{ item.Precio }}</td>\r\n          <td style=\"padding: 8px; text-align: center;\">{{ item.Stock }}</td>\r\n          <td style=\"padding: 8px;\">{{ item.Categoria }}</td>\r\n          <td style=\"padding: 8px; text-align: center;\">{{ item.Activo ? 'Sí' : 'No' }}</td>\r\n          <td style=\"padding: 8px; text-align: center;\">\r\n            <v-btn \r\n              icon=\"mdi-pencil\" \r\n              color=\"blue\" \r\n              size=\"small\" \r\n              variant=\"tonal\"\r\n              rounded=\"lg\"\r\n              class=\"mr-1\"\r\n              @click=\"$emit('editar', item.Id)\">\r\n            </v-btn>\r\n            <v-btn \r\n              icon=\"mdi-delete\" \r\n              color=\"red\" \r\n              size=\"small\" \r\n              variant=\"tonal\"\r\n              rounded=\"lg\"\r\n              @click=\"$emit('eliminar', item.Id)\">\r\n            </v-btn>\r\n          </td>\r\n        </tr>\r\n      </template>\r\n    </v-data-table>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"ProductoTable\",\r\n  props: {\r\n    productos: {\r\n      type: Array,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      busqueda: \"\",\r\n      headers: [\r\n        { title: \"ID\", key: \"Id\", align: \"center\" },\r\n        { title: \"Nombre\", key: \"Nombre\", align: \"start\" },\r\n        { title: \"SKU\", key: \"Sku\", align: \"center\" },\r\n        { title: \"Precio\", key: \"Precio\", align: \"end\" },\r\n        { title: \"Stock\", key: \"Stock\", align: \"center\" },\r\n        { title: \"Categoría\", key: \"Categoria\", align: \"start\" },\r\n        { title: \"Activo\", key: \"Activo\", align: \"center\" },\r\n        { title: \"Acciones\", key: \"actions\", sortable: false, align: \"center\" },\r\n      ]\r\n    }\r\n  },\r\n  computed: {\r\n    filtrados() {\r\n      if (!this.busqueda) return this.productos;\r\n      const term = this.busqueda.toLowerCase();\r\n      return this.productos.filter(p =>\r\n        p.Nombre.toLowerCase().includes(term) ||\r\n        p.Sku.toLowerCase().includes(term) ||\r\n        p.Categoria.toLowerCase().includes(term)\r\n      );\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":";;AAuDA,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE,CACP;QAAEC,KAAK,EAAE,IAAI;QAAEC,GAAG,EAAE,IAAI;QAAEC,KAAK,EAAE;MAAS,CAAC,EAC3C;QAAEF,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAQ,CAAC,EAClD;QAAEF,KAAK,EAAE,KAAK;QAAEC,GAAG,EAAE,KAAK;QAAEC,KAAK,EAAE;MAAS,CAAC,EAC7C;QAAEF,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAM,CAAC,EAChD;QAAEF,KAAK,EAAE,OAAO;QAAEC,GAAG,EAAE,OAAO;QAAEC,KAAK,EAAE;MAAS,CAAC,EACjD;QAAEF,KAAK,EAAE,WAAW;QAAEC,GAAG,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAQ,CAAC,EACxD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,GAAG,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAS,CAAC,EACnD;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,SAAS;QAAEE,QAAQ,EAAE,KAAK;QAAED,KAAK,EAAE;MAAS,CAAC;IAE3E;EACF,CAAC;EACDE,QAAQ,EAAE;IACRC,SAASA,CAAA,EAAG;MACV,IAAI,CAAC,IAAI,CAACP,QAAQ,EAAE,OAAO,IAAI,CAACL,SAAS;MACzC,MAAMa,IAAG,GAAI,IAAI,CAACR,QAAQ,CAACS,WAAW,CAAC,CAAC;MACxC,OAAO,IAAI,CAACd,SAAS,CAACe,MAAM,CAACC,CAAA,IAC3BA,CAAC,CAACC,MAAM,CAACH,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACL,IAAI,KACpCG,CAA