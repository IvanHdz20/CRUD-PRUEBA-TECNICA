{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted } from \"vue\";\nimport { useProductoStore } from \"../stores/productosStore\";\nimport { useRouter, useRoute } from \"vue-router\";\nimport ProductoForm from \"../components/ProductoForm.vue\";\nimport { getProductoPorId } from \"../services/productos\";\nexport default {\n  name: \"EditarProductoView\",\n  components: {\n    ProductoForm\n  },\n  setup() {\n    const store = useProductoStore();\n    const router = useRouter();\n    const route = useRoute();\n    const producto = ref({\n      Nombre: \"\",\n      Sku: \"\",\n      Precio: 0,\n      Stock: 0,\n      Categoria: \"\",\n      Activo: true\n    });\n    const cargarProducto = async () => {\n      const id = route.params.id;\n      const data = await getProductoPorId(id); // Función que llama al backend /:id\n      producto.value = data;\n    };\n    const guardarCambios = async productoEditado => {\n      await store.editarProducto(productoEditado);\n      router.push(\"/\"); // Volver al listado\n    };\n    onMounted(() => {\n      cargarProducto();\n    });\n    return {\n      producto,\n      guardarCambios\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","useProductoStore","useRouter","useRoute","ProductoForm","getProductoPorId","name","components","setup","store","router","route","producto","Nombre","Sku","Precio","Stock","Categoria","Activo","cargarProducto","id","params","data","value","guardarCambios","productoEditado","editarProducto","push"],"sources":["C:\\Users\\Elpap\\OneDrive\\Desktop\\CRUD\\productos-vue\\src\\views\\EditarProductoView.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <h1 class=\"text-h4 mb-4\">Editar Producto</h1>\r\n\r\n    <ProductoForm\r\n      :producto=\"producto\"\r\n      @guardar=\"guardarCambios\"\r\n    />\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from \"vue\";\r\nimport { useProductoStore } from \"../store